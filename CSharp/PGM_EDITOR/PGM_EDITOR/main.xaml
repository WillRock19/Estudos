<Window x:Class="PGM_EDITOR.main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="main" Height="418.905" Width="498.01">
    <Grid Margin="0,56,3.333,3.333">
        <Image x:Name="imgPhoto" HorizontalAlignment="Left" Height="310" Margin="10,10,0,0" VerticalAlignment="Top" Width="468"/>
        <Button x:Name="btnLoad" Content="Load" HorizontalAlignment="Left" Margin="22,-45,0,0" VerticalAlignment="Top" Width="75" Click="btnLoad_Click"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="320" Margin="10,10,0,0" VerticalAlignment="Top" Width="468"/>
        <Button x:Name="btnRotateR" Content="RotateRight" HorizontalAlignment="Left" Margin="205,-45,0,0" VerticalAlignment="Top" Width="75" Click="btnRotateR_Click"/>
        <Button x:Name="btnRotateL" Content="RotateLeft" HorizontalAlignment="Left" Margin="125,-45,0,0" VerticalAlignment="Top" Width="75" Click="btnRotateL_Click"/>
        <Button x:Name="btnUndo" Content="Undo" HorizontalAlignment="Left" Margin="344,-45,0,0" VerticalAlignment="Top" Width="75" Click="btnUndo_Click"/>
    </Grid>
</Window>
